@model IEnumerable<TicketV2.Models.Ticket>

@{
    ViewBag.Title = "Index";
}

<h2>Tickets</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Created
        </th>
        <th>
            User id
        </th>
        <th>
            Organisation
        </th>
        <th>
            Contact EU
        </th>
        <th>
            Contact
        </th>
        <th>
            Contact Tel.
        </th>
        <th>
            Contact Email
        </th>
        <th>
            Status
        </th>
        <th>
            Category
        </th>
        <th>
            Description
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactOrg)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactEU)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactTel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContactEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TicketID }) |
            @Html.ActionLink("Details", "Details", new { id=item.TicketID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TicketID }) |
            @Html.ActionLink("Comments", "Index", "Details", new { id = item.TicketID }, null)
        </td>
    </tr>
}

</table>
